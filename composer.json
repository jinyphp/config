{
    "name": "jiny/config",
    "description": "config for jinyPHP",
    "keywords": [
        "jinyPHP",
        "framwork",
        "config",
        "Laravel"
    ],
    "homepage": "http://www.jinyphp.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com",
            "homepage": "http://hojin.io"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "symfony/yaml": "^7.0"
